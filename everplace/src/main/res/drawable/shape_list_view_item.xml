<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    
    <item android:state_pressed="false"> 
        <shape>
            <solid
        		android:color="#697289" />
            <stroke
                android:width="0dp"
                android:color="#697289" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    
    <item android:state_pressed="true"> 
        <shape>
            <solid
        		android:color="#7d89a6" />
            <stroke
                android:width="0dp"
                android:color="#7d89a6" />
            <corners
                android:radius="5dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    
</selector>